##
# git checkout from branch -a list.
#
local res
local branch=$(git branch -a | peco | tr -d ' ')
if [ -n "$branch" ]; then
  if [[ "$branch" =~ "remotes/" ]]; then
    local b=$(echo $branch | awk -F'/' '{print $3}')
    res="git checkout -b ${b} ${branch}"
  else
    res"git checkout ${branch}"
  fi
fi

_cool-peco-on-complete $res
